{
  "name": "gulp-flex-svg",
  "version": "0.1.4",
  "description": "flex-svg plugin for gulp",
  "main": "build/index-es5.js",
  "scripts": {
    "lint": "jshint *.js --reporter node_modules/jshint-stylish/stylish",
    "transpile": "rimraf build && traceur --experimental --out build --script *.js",
    "rename": "renamer --find '.' --replace '-es5.' build/*.js",
    "build": "npm run-script transpile && npm run-script rename",
    "pretest": "npm run-script lint & npm run-script build",
    "test": "mocha build/test-es5.js --reporter spec --timeout 100",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/gulp-flex-svg.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/gulp-flex-svg/issues"
  },
  "homepage": "https://github.com/shinnn/gulp-flex-svg",
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "flex-svg": "^0.2.0",
    "gulp-util": "~2.2.14",
    "through2": "^1.0.0"
  },
  "devDependencies": {
    "event-stream": "^3.1.5",
    "jshint": "^2.5.1",
    "jshint-stylish": "^0.2.0",
    "mocha": "^1.18.2",
    "renamer": "^0.4.3",
    "rimraf": "^2.2.8",
    "traceur": "0.0.32",
    "vinyl": "~0.2.3"
  }
}
