{
  "name": "gulp-flex-svg",
  "version": "0.1.2",
  "description": "flex-svg plugin for gulp",
  "main": "index.js",
  "scripts": {
    "build-main": "traceur --experimental --out test.js --script test-es6.js",
    "build-test": "traceur --experimental --out index.js --script index-es6.js",
    "build": "npm run-script build-main && npm run-script build-test",
    "test": "npm run-script build && mocha test.js --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/gulp-flex-svg.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/gulp-flex-svg/issues"
  },
  "homepage": "https://github.com/shinnn/gulp-flex-svg",
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "through2": "~0.4.1",
    "flex-svg": "~0.0.4",
    "gulp-util": "~2.2.14"
  },
  "devDependencies": {
    "vinyl": "~0.2.3",
    "event-stream": "~3.1.0",
    "mocha": "~1.17.1",
    "traceur": "0.0.25"
  }
}
