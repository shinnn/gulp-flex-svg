{
  "name": "gulp-flex-svg",
  "version": "0.1.3",
  "description": "flex-svg plugin for gulp",
  "main": "./index-es5.js",
  "scripts": {
    "lint": "jshint index.js test.js --reporter node_modules/jshint-stylish/stylish",
    "build-main": "traceur --experimental --out test-es5.js --script test.js",
    "build-test": "traceur --experimental --out index-es5.js --script index.js",
    "build": "npm run-script build-main && npm run-script build-test",
    "pretest": "npm run-script lint && npm run-script build",
    "test": "mocha test-es5.js --reporter spec",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/gulp-flex-svg.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/gulp-flex-svg/issues"
  },
  "homepage": "https://github.com/shinnn/gulp-flex-svg",
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "flex-svg": "^0.2.0",
    "gulp-util": "~2.2.14",
    "through2": "^1.0.0"
  },
  "devDependencies": {
    "event-stream": "^3.1.1",
    "mocha": "^1.18.2",
    "traceur": "0.0.32",
    "vinyl": "~0.2.3",
    "jshint-stylish": "^0.1.5",
    "jshint": "^2.5.0"
  }
}
