{
  "name": "gulp-flex-svg",
  "version": "0.1.4",
  "description": "flex-svg plugin for gulp",
  "main": "build/index-es5.js",
  "scripts": {
    "lint": "jshint *.js --reporter node_modules/jshint-stylish/stylish",
    "build-main": "traceur --experimental --out build/index-es5.js --script index.js",
    "build-test": "traceur --experimental --out build/test-es5.js --script test.js",
    "pretest": "npm run lint & rimraf build && npm run build-main && npm run build-test",
    "test": "mocha build/test-es5.js --reporter spec --timeout 100"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinnn/gulp-flex-svg.git"
  },
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/gulp-flex-svg/issues"
  },
  "homepage": "https://github.com/shinnn/gulp-flex-svg",
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "flex-svg": "^0.2.0",
    "gulp-util": "^2.2.17",
    "through2": "^1.0.0"
  },
  "devDependencies": {
    "jshint": "^2.5.1",
    "jshint-stylish": "^0.2.0",
    "mocha": "^1.20.1",
    "require-main": "^0.1.0",
    "rimraf": "^2.2.8",
    "traceur": "0.0.32",
    "vinyl": "~0.2.3"
  }
}
