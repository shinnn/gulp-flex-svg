  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinnn/gulp-flex-svg/issues"
  },
  "homepage": "https://github.com/shinnn/gulp-flex-svg",
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "flex-svg": "^0.1.1",
    "gulp-util": "~2.2.14",
    "through2": "^1.0.0"
  },
  "devDependencies": {
    "event-stream": "^3.1.1",
