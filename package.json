{
  "name": "gulp-flex-svg",
  "version": "1.0.0",
  "description": "flex-svg plugin for gulp",
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "_mocha test.js",
    "test-dot": "_mocha test.js --reporter dot",
    "coverage": "istanbul cover _mocha -- test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/gulp-flex-svg",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "files": [
    "index.js",
    "LICENSE"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/gulp-flex-svg/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "gulpplugin",
    "svg",
    "flexible",
    "responsive",
    "width",
    "height"
  ],
  "dependencies": {
    "bufferstreams": "0.0.2",
    "flex-svg": "^ 2.0.0",
    "gulp-util": "^3.0.1",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "eslint": "^0.11.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "mocha": "^2.1.0",
    "simple-bufferstream": "0.0.4",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "requireBlocksOnNewline": true,
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
